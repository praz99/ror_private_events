<h1>Attend Event</h1>
<%= form_for [@event, @event.event_attendings.build] do |f| %>
    <% User.all.each do |user| %>
      <p>
        <%= f.check_box :event_attendee_id, { multiple: true }, user.id, false %>
        <%= f.label user.name %>
      </p>
    <% end %>
    <%= f.submit "Invite Users" %>
<% end %>